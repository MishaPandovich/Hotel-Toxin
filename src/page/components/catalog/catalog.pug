main.catalog-page
  include ../sidebar/sidebar.pug
  section.catalog
    h2.catalog__title Номера, которые мы для вас подобрали
    ul.cards
      li.card
        div.card-slider
          div.card-slider__item
            a.card-slider__link(href="/mock-address/change-me")
              img(src="#" alt="Первый слайд")
          div.card-slider__item
            a.card-slider__link(href="/mock-address/change-me")
              img(src="#" alt="Второй слайд")
          div.card-slider__item
            a.card-slider__link(href="/mock-address/change-me")
              img(src="#" alt="Третий слайд")
